{
  "manifest_version": 2,
  "name": "Outlook Mail Ads Remover",
  "description": "Remove adspace for Outlook Mail",
  "version": "1.1.0",
  "minimum_chrome_version": "40",
  "icons": {
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://outlook.live.com/mail/*"
      ],
      "js": [
        "fix.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "https://outlook.live.com/mail/"
  ]
}
